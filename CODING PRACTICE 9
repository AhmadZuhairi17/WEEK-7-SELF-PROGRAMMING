package mySelfProgramming;

class Practice9 {

	public static void main(String[] args) {
		int height=5, width=5;
		
		for ( int i = 0 ; i < height ; i++ )
		   {
		      // PRINT a row
		      for ( int i1 = 0 ; i1 < width ; i1++ )
		      {
		         System.out.print( "*" ) ;
		      }
		      // PRINT newline
		      System.out.println() ;
		   }
	}

}
